APP_NAME="Inteligência MAX"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://inteligenciamax.com.br

# Modo demonstração (permite visualizar interface sem WhatsApp conectado)
CHATBOT_DEMO_MODE=true

# ============================================================================
# CONFIGURAÇÕES DE TEMA E BRANDING
# ============================================================================
# Personalize a identidade visual completa da plataforma

# Cores Primárias
APP_PRIMARY_COLOR=#29B6F6
APP_SECONDARY_COLOR=#004AAD
APP_ACCENT_COLOR=#FF6600

# Cores de Suporte (Status)
APP_SUCCESS_COLOR=#28a745
APP_WARNING_COLOR=#ffc107
APP_DANGER_COLOR=#dc3545
APP_INFO_COLOR=#17a2b8

# Gradientes
APP_GRADIENT_START=#29B6F6
APP_GRADIENT_END=#039BE5

# Logotipos e Ícones
APP_LOGO_LIGHT=assets/images/logo_icon/logo.png
APP_LOGO_DARK=assets/images/logo_icon/logo_dark.png
APP_LOGO_ICON=assets/images/logo_icon/favicon.png
APP_FAVICON=assets/images/logo_icon/favicon.png

# Dimensões do Logo
APP_LOGO_HEIGHT=50px
APP_LOGO_WIDTH=auto

# Tipografia
APP_FONT_PRIMARY="Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif"
APP_FONT_SECONDARY="Montserrat, sans-serif"
APP_FONT_HEADING="Poppins, sans-serif"
APP_FONT_SIZE=14px
APP_FONT_SIZE_HEADING=24px

# Design System
APP_BORDER_RADIUS=8px
APP_BORDER_RADIUS_LARGE=16px
APP_BOX_SHADOW="0 2px 8px rgba(0,0,0,0.1)"
APP_BOX_SHADOW_HOVER="0 4px 16px rgba(0,0,0,0.15)"
APP_SPACING_UNIT=8px

# Layout
APP_SIDEBAR_WIDTH=260px
APP_TOPBAR_HEIGHT=70px

# Modo Escuro
APP_DARK_MODE_ENABLED=true
APP_DARK_PRIMARY_COLOR=#1a1a1a
APP_DARK_SECONDARY_COLOR=#2d2d2d

# Animações
APP_ANIMATION_DURATION=0.3s
APP_ANIMATION_EASING=ease-in-out

# ============================================================================

